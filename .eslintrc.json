{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "prefer-arrow-callback": "off",
        "operator-linebreak": "off",
        "func-names": "off",
        "no-use-before-define": "off",
        "no-plusplus": "off"
    }
}
